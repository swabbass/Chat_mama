<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme for API 11+. This theme completely replaces
        AppBaseTheme from res/values/styles.xml on API 11+ devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Holo.Light">
        <!-- API 11 theme customizations can go here. -->
    </style>
    <style name="MyActionBar" parent="android:Theme.Holo.Light">
        <item name="android:background">#0d97fb</item>
        <item name="android:titleTextStyle">@style/TitleColor</item>
    </style>

    <style name="TitleColor" parent="android:Theme.Holo.Light">
        <item name="android:textColor">#ffffff</item>
    </style>

</resources>
